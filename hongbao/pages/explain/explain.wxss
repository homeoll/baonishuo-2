page{
  background: #F2F2F2;
  box-sizing: border-box;
  padding: 20rpx 40rpx 30rpx;
}
.title{
  width: 100%;
  height: 100rpx;
  text-align: center;
  font-size: 40rpx;
  line-height:100rpx;
  color: #6C6C6C;
}
.content{
  width: 100%;
  height: auto;
  font-size: 34rpx;
  color:#939393;
  text-align:justify;
}
button{
  width: 100%;
  height: 84rpx;
  line-height: 84rpx;
  margin-top: 100rpx;
  background-color: #D95940;
  color:#fff;
}
.foot image{
  width: 44rpx;
  height:44rpx;
  vertical-align: text-bottom;
  margin-right: 10rpx;
}
.foot span{
  font-size: 32rpx;
}
.jubao{
  width:100%;
  height: auto;

  margin-top: 70rpx;
  margin-bottom: 10rpx;
  text-align: center;
}
.jubao span{
  font-size:24rpx;
  display:inline-block;
  margin-right: 10rpx;
  line-height: 30rpx;
}
