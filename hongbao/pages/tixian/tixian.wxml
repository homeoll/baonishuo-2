<view class='jubao'>
  <span bindtap='gojubao'>举报</span>
  <span style='margin-left:10rpx;margin-right:10rpx;'> | </span>
  <span class='zi1' bindtap='gopartner'>成为合伙人</span>
  <view class='error'>提现金额超账户余额</view>
</view>
<view class='neirong'>
